{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "require": {
    "php": "~7.3",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "aws/aws-sdk-php-symfony": "^2.0",
    "behat/transliterator": "^1.2",
    "centrifugal/phpcent": "~3.0",
    "doctrine/annotations": "^1.6",
    "doctrine/doctrine-bundle": "~1.10",
    "doctrine/doctrine-migrations-bundle": "~2.0",
    "doctrine/orm": "~2.6",
    "friendsofsymfony/elastica-bundle": "^5.0",
    "gesdinet/jwt-refresh-token-bundle": "~0.5",
    "guzzlehttp/guzzle": "~6.3",
    "influxdb/influxdb-php": "~1.14",
    "jurosh/pdf-merge": "^2.0",
    "league/tactician-bundle": "~1.1",
    "league/tactician-doctrine": "~1.1",
    "lexik/jwt-authentication-bundle": "~2.6",
    "lstrojny/functional-php": "^1.9",
    "mark-gerarts/automapper-plus-bundle": "~1.1",
    "minishlink/web-push": "~5.1",
    "nelmio/api-doc-bundle": "~3.3.0",
    "nelmio/cors-bundle": "^1.5",
    "opsway/doctrine-dbal-postgresql": "~0.1",
    "php-amqplib/rabbitmq-bundle": "^1.14",
    "php-http/guzzle6-adapter": "^2.0",
    "ralouphie/mimey": "^2.1",
    "ramsey/uuid": "^3.8",
    "rguedes/pdfmerger": "^1.0",
    "sensio/framework-extra-bundle": "^5.2",
    "stof/doctrine-extensions-bundle": "~1.3",
    "symfony/asset": "~4.2",
    "symfony/cache": "~4.2",
    "symfony/console": "~4.2",
    "symfony/dependency-injection": "~4.2",
    "symfony/dotenv": "~4.1",
    "symfony/expression-language": "~4.2",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "~4.2",
    "symfony/monolog-bundle": "^3.3",
    "symfony/process": "4.2.*",
    "symfony/security": "~4.2",
    "symfony/serializer": "~4.2",
    "symfony/swiftmailer-bundle": "~3.2",
    "symfony/templating": "~4.2",
    "symfony/translation": "~4.2",
    "symfony/validator": "4.2.*",
    "symfony/workflow": "4.2.*",
    "symfony/yaml": "~4.2",
    "wapmorgan/morphos": "^3.2.18"
  },
  "require-dev": {
    "codeception/codeception": "^3.1",
    "doctrine/doctrine-fixtures-bundle": "^3.1",
    "flow/jsonpath": "^0.4.0",
    "league/factory-muffin": "^3.0",
    "league/factory-muffin-faker": "^2.1",
    "neronmoon/teamcity-codeception": "^0.1.0",
    "phpstan/phpstan-symfony": "^0.12.4",
    "symfony/debug-pack": "*",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*",
    "symfony/web-server-bundle": "4.2.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Slon\\MetricBundle\\": "third-party/metric-bundle/src/",
      "Slon\\Metric\\": "third-party/metric/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpstan": [
      "./vendor/bin/phpstan analyse -v --level=0 --memory-limit=512m -c phpstan.neon src tests"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.2.*"
    }
  }
}
